{
    "name": "amethyst/ftp",
    "keywords": ["laravel", "amethyst", "amethyst-ftp", "ftp"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "amethyst/core": "0.2.*",
        "amethyst/data-builder": "0.2.*",
        "amethyst/file-generator": "0.2.*",
        "amethyst/exporter": "0.2.*",
        "nicolab/php-ftp-client": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*",
        "amethyst/user": "0.2.*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\FtpServiceProvider"
            ]
        }
    }
}
