{
    "name": "railken/amethyst-ftp",
    "keywords": ["laravel", "amethyst", "amethyst-ftp", "ftp"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "railken/amethyst-common": "0.1.*",
        "railken/amethyst-data-builder": "0.1.*",
        "railken/amethyst-file-generator": "0.1.*",
        "nicolab/php-ftp-client": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "3.6.*"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\Amethyst\\Providers\\FtpServiceProvider"
            ]
        }
    }
}
